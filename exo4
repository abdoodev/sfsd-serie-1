#include<stdio.h>
#include<stdlib.h>  
#include<string.h>
int main(){
    FILE *f =fopen("txt.txt","w");
    if ( f== NULL){
        printf("error");
        exit(1);
    }
    int line =0;
    int c=fgetc(f);
    while (c!=EOF){
       if (c=='\n'){
              line++;
            }
    }
    printf("Number of lines: %d\n", line);
    fclose(f);
    return 0;
}
